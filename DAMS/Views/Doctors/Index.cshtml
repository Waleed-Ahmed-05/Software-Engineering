@model IEnumerable<DAMS.Models.Doctor>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.User_ID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Clinic_Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Location)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Certification)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Study_Field)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Availability)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Offline_Duration_Start)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Offline_Duration_End)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Online_Duration_Start)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Online_Duration_End)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.User_ID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Clinic_Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Location)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Certification)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Study_Field)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Availability)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Offline_Duration_Start)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Offline_Duration_End)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Online_Duration_Start)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Online_Duration_End)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Doctor_ID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Doctor_ID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Doctor_ID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
